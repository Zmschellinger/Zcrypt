<%= form_with (model: encryptionLog, local: true) do |form| %>
    
    
    
    <div> 
        <%= form.lable :filename%><br>
        <%= form.text_field :filename%>
        <% encryption_logs.errors.full_messages_for(:filename).each do |message|%>
            <div><%= message %></div>
        <% end %>
    </div>

    <div>
        <%= form.label :body %><br>
        <%= form.text_area :body %><br>
        <% article.errors.full_messages_for(:body).each do |message| %>
        <div><%= message %></div>
        <% end %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>
